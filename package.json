{
  "name": "@editorjs/marker",
  "version": "1.4.0",
  "keywords": [
    "codex editor",
    "marker",
    "editor.js",
    "editorjs"
  ],
  "description": "Marker Tool for Editor.js",
  "license": "MIT",
  "repository": "https://github.com/editor-js/marker",
  "files": [
    "dist"
  ],
  "main": "./dist/marker.umd.js",
  "module": "./dist/marker.mjs",
  "exports": {
    ".": {
      "import": "./dist/marker.mjs",
      "require": "./dist/marker.umd.js"
    }
  },
  "scripts": {
    "dev": "onchange 'src/*.js' 'src/*.css' -- npm run build",
    "build": "vite build"
  },
  "author": {
    "name": "CodeX",
    "email": "team@codex.so"
  },
  "devDependencies": {
    "onchange": "^7.1.0",
    "vite": "^4.5.0",
    "vite-plugin-css-injected-by-js": "^3.3.0"
  },
  "dependencies": {
    "@codexteam/icons": "^0.0.5"
  }
}
